@article{adachi_joint_2009,
	title = {Joint {Procrustes} {Analysis} for {Simultaneous} {Nonsingular} {Transformation} of {Component} {Score} and {Loading} {Matrices}},
	volume = {74},
	issn = {0033-3123, 1860-0980},
	url = {http://link.springer.com/article/10.1007/s11336-009-9131-2},
	doi = {10.1007/s11336-009-9131-2},
	abstract = {In component analysis solutions, post-multiplying a component score matrix by a nonsingular matrix can be compensated by applying its inverse to the corresponding loading matrix. To eliminate this indeterminacy on nonsingular transformation, we propose Joint Procrustes Analysis (JPA) in which component score and loading matrices are simultaneously transformed so that the former matrix matches a target score matrix and the latter matches a target loading matrix. The loss function of JPA is a function of the nonsingular transformation matrix and its inverse, and is difficult to minimize straightforwardly. To deal with this difficulty, we reparameterize those matrices by their singular value decomposition, which reduces the minimization to alternately solving quartic equations and performing the existing multivariate procedures. This algorithm is assessed in a simulation study. We further extend JPA for cases where targets are linear functions of unknown parameters. We also discuss how the application of JPA can be extended in different fields.},
	language = {en},
	number = {4},
	urldate = {2014-08-07},
	journal = {Psychometrika},
	author = {Adachi, Kohei},
	month = dec,
	year = {2009},
	keywords = {Assessment, Testing and Evaluation, component analysis, joint Procrustes analysis, nonsingular transformation, Psychometrics, singular value decomposition, Statistical Theory and Methods, Statistics for Social Science, Behavorial Science, Education, Public Policy, and Law},
	pages = {667--683},
	file = {Snapshot:/Users/mark/Library/Application Support/Firefox/Profiles/vg4kuy5x.default/zotero/storage/JFMV4H4M/10.html:text/html}
}

@article{stam_personal-construct_1998,
	title = {Personal-construct theory and social constructionism: {Difference} and dialogue},
	volume = {11},
	issn = {1072-0537},
	shorttitle = {Personal-construct theory and social constructionism},
	url = {http://www.tandfonline.com/doi/abs/10.1080/10720539808405220},
	doi = {10.1080/10720539808405220},
	abstract = {Abstract In psychology there are at least as many varieties of social constructionism (SC) as there are of constructivism. Many of these versions of SC share a number of crucial properties with several versions of constructivism, including epistemological and ontological asssumptions that are not articulated often or clearly. Personal construct theory (PCT), one version of constructivism, appears in its negative identity to reject the same positivist and representationist psychologies so strongly eschewed by SC. The positive programs of SC and PCT are in accord insofar as they attempt to establish notions of knowledge and categories of psychological life as inherently constructed. The two programs depart however on the crucial notion of the origins of such categories. Linguistic and social communities are essentially the repositories and generators of knowledge even as we are individual knowers. But the knowledge we as individual knowers construct makes sense only within the communal use of categories of knowing, feeling, and construing. On this account we are ?persons in conversation? and our perceived unities emerge only as discursive practices conforming to local cultural norms.
