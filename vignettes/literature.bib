@article{cadoret_construction_2013,
	title = {Construction and evaluation of confidence ellipses applied at sensory data},
	volume = {28},
	issn = {0950-3293},
	url = {http://www.sciencedirect.com/science/article/pii/S0950329312001887},
	doi = {10.1016/j.foodqual.2012.09.005},
	abstract = {The sensory evaluation of products by a panel of experts or consumers involves qualitative data (in sorting task description), quantitative variables structured by groups (in napping or in free choice profiling), quantitative and qualitative data (in sorted napping). Whatever the nature of data, a mean configuration of products is built and it is crucial to assess its stability, especially when the judges are untrained. Unfortunately, most of the confidence ellipses constructed around the position of the products do not give a confidence area in the sense that two products are significantly different when their ellipses do not overlap. Indeed, most ellipses proposed in the literature are too small and lead the user to misinterpretations since they are too optimistic. Here we propose to use truncated total bootstrap to build confidence ellipses that can be actually interpreted as confidence areas. The ellipses are evaluated from simulations and real examples and the evaluation shows that the methodology is adequate for all methods.},
	number = {1},
	urldate = {2016-03-29},
	journal = {Food Quality and Preference},
	author = {Cadoret, Marine and Husson, François},
	month = apr,
	year = {2013},
	keywords = {Confidence ellipses, Napping, Product description, QDA, Sorting task, Truncated total bootstrap},
	pages = {106--115},
	file = {ScienceDirect Snapshot:/Users/mark/Library/Application Support/Firefox/Profiles/vg4kuy5x.default/zotero/storage/J3NS9BHE/S0950329312001887.html:text/html}
}

